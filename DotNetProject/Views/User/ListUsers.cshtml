@model List<Microsoft.AspNetCore.Identity.IdentityUser>

    @{
    ViewBag.Title= "All Users";
    Layout = "~/Views/Shared/AdminLTE/_Layout.cshtml";
    }

    <h1>All Users</h1>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
             {
            <tr>
                <td>@user.Id</td>
                <td>@user.Email</td>

            </tr>
              }
        </tbody>
    </table>

